{
  "expo": {
    "name": "Red Food",
    "slug": "red-food-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSUserNotificationUsageDescription": "We need to send you notifications about your orders and updates."
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "notifications": {
        "icon": "./assets/notificationicon.png",
        "color": "#ffffff",
        "sounds": [
          "./assets/notificationsound.wav"
        ],
        "androidMode": "default",
        "androidCollapsedTitle": "Red Food Notification"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "VIBRATE",
        "SCHEDULE_EXACT_ALARM",
        "NOTIFICATIONS",
        "POST_NOTIFICATIONS"
      ],
      "package": "com.endernitish.redfoodclient"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "@react-native-firebase/app",
        {
          "googleServiceFile": "./google-services.json"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep public class com.horcrux.svg.** {*;}"
          }
        }
      ],
      [
        "@react-native-firebase/messaging",
        {
          "presentationOptions": [
            "badge",
            "sound",
            "alert"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "./plugins/phonepe-plugin.js"
      ],
      [
        "./plugins/notification-plugin.js"
      ],
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e5ea6302-cbff-4f6e-9d1d-19f85209cfe7"
      }
    }
  }
}
